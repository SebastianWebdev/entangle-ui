{
  "name": "entangle-ui",
  "version": "0.5.0",
  "type": "module",
  "sideEffects": [
    "*.css",
    "*.css.ts"
  ],
  "description": "A specialized React component library for building professional editor interfaces",
  "main": "dist/esm/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "default": "./dist/esm/index.js"
    },
    "./palettes": {
      "types": "./dist/types/palettes.d.ts",
      "default": "./dist/esm/palettes.js"
    },
    "./theme": {
      "types": "./dist/types/theme/darkTheme.css.d.ts",
      "default": "./dist/esm/theme/darkTheme.css.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "storybook dev -p 6006",
    "build": "rollup -c",
    "build-storybook": "storybook build",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:failed": "vitest run --reporter=./scripts/failed-tests-reporter.js",
    "test:coverage": "vitest run --coverage",
    "test:package-report": "npm run test:coverage && node scripts/generate-package-report.js",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "type-check": "tsc --noEmit",
    "clean": "node -e \"require('fs').rmSync('dist', {recursive: true, force: true})\"",
    "prepublishOnly": "npm run clean && npm run build",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "format:staged": "prettier --write --cache --ignore-unknown",
    "storybook": "storybook dev -p 6006",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "changeset publish",
    "size": "size-limit"
  },
  "keywords": [
    "react",
    "typescript",
    "ui-library",
    "editor",
    "components",
    "ui-toolkit",
    "design-system",
    "react-components",
    "editor-ui"
  ],
  "author": "Sebastian Gołębiowski <see.golebiowski@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/SebastianWebdev/entangle-ui#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SebastianWebdev/entangle-ui.git"
  },
  "bugs": {
    "url": "https://github.com/SebastianWebdev/entangle-ui/issues"
  },
  "peerDependencies": {
    "@base-ui/react": "^1.1.0",
    "@floating-ui/react": "^0.27.17",
    "react": ">=19.1.0",
    "react-dom": ">=19.1.0"
  },
  "dependencies": {
    "@vanilla-extract/dynamic": "^2.1.5",
    "@vanilla-extract/recipes": "^0.5.7"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.2",
    "@changesets/cli": "^2.29.8",
    "@eslint/js": "^9.39.2",
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "@size-limit/preset-small-lib": "^12.0.0",
    "@storybook/addon-docs": "^10.2.8",
    "@storybook/addon-onboarding": "^10.2.8",
    "@storybook/addon-themes": "^10.2.8",
    "@storybook/react-vite": "^10.2.8",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.2.13",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.55.0",
    "@typescript-eslint/parser": "^8.55.0",
    "@vanilla-extract/css": "^1.18.0",
    "@vanilla-extract/rollup-plugin": "^1.5.1",
    "@vanilla-extract/vite-plugin": "^5.1.4",
    "@vitejs/plugin-react": "^4.5.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/eslint-plugin": "^1.6.7",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.39.2",
    "eslint-plugin-storybook": "^10.2.8",
    "globals": "^16.5.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.8.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "rollup": "^4.57.1",
    "rollup-plugin-dts": "^6.3.0",
    "rollup-plugin-preserve-directives": "^0.4.0",
    "size-limit": "^12.0.0",
    "storybook": "^10.2.8",
    "typescript": "^5.9.3",
    "vite": "^6.3.5",
    "vitest": "^3.2.4"
  },
  "overrides": {}
}
