{
  "name": "entangle-ui",
  "version": "0.2.0",
  "type": "module",
  "description": "A specialized React component library for building professional editor interfaces",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "storybook dev -p 6006",
    "build": "rollup -c",
    "build-storybook": "storybook build",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:failed": "vitest run --reporter=./scripts/failed-tests-reporter.js",
    "test:coverage": "vitest run --coverage",
    "test:package-report": "npm run test:coverage && node scripts/generate-package-report.js",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "type-check": "tsc --noEmit",
    "clean": "node -e \"require('fs').rmSync('dist', {recursive: true, force: true})\"",
    "prepublishOnly": "npm run clean && npm run build",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "format:staged": "prettier --write --cache --ignore-unknown",
    "storybook": "storybook dev -p 6006",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "changeset publish"
  },
  "keywords": [
    "react",
    "typescript",
    "ui-library",
    "editor",
    "components",
    "ui-toolkit",
    "design-system",
    "react-components",
    "editor-ui"
  ],
  "author": "Sebastian Gołębiowski <see.golebiowski@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/SebastianWebdev/entangle-ui#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SebastianWebdev/entangle-ui.git"
  },
  "bugs": {
    "url": "https://github.com/SebastianWebdev/entangle-ui/issues"
  },
  "peerDependencies": {
    "@emotion/react": "^11.0.0",
    "@emotion/styled": "^11.0.0",
    "react": ">=19.1.0",
    "react-dom": ">=19.1.0"
  },
  "dependencies": {
    "@base-ui-components/react": "^1.0.0-beta.0",
    "@emotion/react": "^11.0.0",
    "@emotion/styled": "^11.0.0"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.2",
    "@changesets/cli": "^2.29.8",
    "@eslint/js": "^9.27.0",
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-typescript": "^12.1.2",
    "@storybook/addon-docs": "^9.0.6",
    "@storybook/addon-onboarding": "^9.0.6",
    "@storybook/addon-themes": "^9.0.6",
    "@storybook/react-vite": "^9.0.6",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^19.1.5",
    "@types/react-dom": "^19.1.5",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "@vitejs/plugin-react": "^4.5.0",
    "@vitest/coverage-v8": "^3.1.4",
    "@vitest/eslint-plugin": "^1.2.1",
    "@vitest/ui": "^3.1.4",
    "eslint": "^9.27.0",
    "eslint-plugin-storybook": "^9.0.6",
    "globals": "^16.2.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.5.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "rollup": "^4.41.0",
    "rollup-plugin-dts": "^6.2.1",
    "storybook": "^9.0.6",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vitest": "^3.1.4"
  },
  "overrides": {}
}
